{
  "name": "Disgraced",
  "version": "0.0.0",
  "description": "Website for the Icelandic adaption of the Pulitzer-winning play Disgraced",
  "license": "UNLICENSED",
  "homepage": "https://github.com/benediktvaldez/Disgraced",
  "repository": "benediktvaldez/Disgraced",
  "author": {
    "name": "Benedikt D Valdez",
    "email": "hello@bdvs.info",
    "url": "http://bdvs.info"
  },
  "contributors": [],
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "build": "gulp --gulpfile gulp/gulpfile.babel.js build",
    "server:gulp": "gulp --gulpfile gulp/gulpfile.babel.js server",
    "serve": "http-server -p 3000 --utc --robots -c-1 -a localhost app",
    "start": "run-s build serve",
    "dev": "supervisor --quiet --watch gulp --no-restart-on exit --exec npm -- run server:gulp",
    "lint": "eslint --fix gulp src test",
    "test": "nyc ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^5.8.38",
    "babelify": "^7.3.0",
    "browser-sync": "^2.12.7",
    "browserify": "^13.0.1",
    "connect-history-api-fallback": "^1.2.0",
    "contentful": "^3.4.3",
    "coveralls": "^2.11.9",
    "es6-promise": "^3.2.1",
    "eslint": "^2.9.0",
    "eslint-config-icelandair": "^1.0.0",
    "eslint-plugin-import": "^1.8.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-eslint": "^2.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-pug": "^3.0.2",
    "gulp-sass": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "http-server": "^0.9.0",
    "lodash": "^4.12.0",
    "lodash.assign": "^4.0.8",
    "marked": "^0.3.5",
    "npm-run-all": "^2.2.2",
    "nyc": "^6.4.4",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "supervisor": "^0.10.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "es2015"
      ],
      "plugins": [
        "add-module-exports"
      ],
      "sourceMaps": "inline"
    }
  }
}
